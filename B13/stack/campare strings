1--ab#d---s
2--ac#d----t

stack s1;
stack s2;

for(int i=0;i<n;i++)
{
if(s[i] !='#')
  s1.push(s[i]);
  
  if(t[i] !='#')
   s2.push(t[i]);
   
   if(s[i] =='#') s1.pop();
    if(t[i] =='#'). s2.pop();
   }
    while( s1.empty()==False && s2.empty()==False)
    {
    if(s1.top()==s2.top())
    {
    s1.pop();
    s2.pop();
    }
    else {return false;
    }
    }
    
    
  if(s1.empty()==True && s2.empty()==true)
   return true;
   else
   return false;
   
   }
   
   
   
  
 
